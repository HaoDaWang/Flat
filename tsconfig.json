{
  "compilerOptions": {
    "target": "esnext",                                  
    "experimentalDecorators": true,                   
    "module": "commonjs",                                
    "moduleResolution": "nodenext",
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "noImplicitAny": false,                            
    "skipLibCheck": true,
  },
}
